{
  "name": "intern-admin-collaboration-server",
  "version": "1.0.0",
  "description": "intern management",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/server.mjs",
    "lint": "eslint --ignore-path .eslintignore --ext .mjs .",
    "lint-check": "eslint . --fix",
    "format": "prettier --ignore-path .gitignore --write . \"**/*.+(mjs|js|json)\"",
    "prettier:check": "prettier --write .",
    "prepare": "node .husky/install.mjs"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint:check",
      "npm run prettier:check",
      "npm run format",
      "lint"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "mongoose": "^8.1.1",
    "nodemon": "^3.0.3"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5"
  }
}
